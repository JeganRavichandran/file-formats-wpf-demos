<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <UseWindowsForms>true</UseWindowsForms>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>ExceltoPDF</AssemblyName>
    <RootNamespace>ExceltoPDF</RootNamespace>
    <DefineConstants>TRACE;NETCORE</DefineConstants>
    <EnableDefaultApplicationDefinition>false</EnableDefaultApplicationDefinition>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="App.xaml.cs" />
    <Compile Remove="Window.xaml.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="App.xaml" />
    <None Remove="Window.xaml" />
    <None Remove="Fonts\bahnschrift.ttf" />
    <None Remove="Fonts\georgiab.ttf" />
  </ItemGroup>
  <ItemGroup>
    <Page Remove="App.xaml" />
    <Page Remove="Window.xaml" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Fonts\bahnschrift.ttf" />
    <EmbeddedResource Include="Fonts\georgiab.ttf" />
  </ItemGroup>
  <ItemGroup>
    <ApplicationDefinition Include="App1.xaml" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Syncfusion.Compression.Base" Version="18.1.0.42" />
    <PackageReference Include="Syncfusion.ExcelToPdfConverter.Wpf" Version="18.1.0.42" />
    <PackageReference Include="Syncfusion.Pdf.Wpf" Version="18.1.0.42" />
    <PackageReference Include="Syncfusion.XlsIO.Wpf" Version="18.1.0.42" />
    <PackageReference Include="Syncfusion.ExcelChartToImageConverter.WPF" Version="18.1.0.42" />
    <PackageReference Include="Syncfusion.SfChart.WPF" Version="18.1.0.42" />
    <PackageReference Include="Syncfusion.Shared.WPF" Version="18.1.0.42" />
    <PackageReference Include="Syncfusion.Licensing" Version="18.1.0.42" />
  </ItemGroup>
  <ItemGroup>
    <Resource Include="App.ico" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
  </ItemGroup>
  <ItemGroup>
    <None Update="App1.xaml">
      <Generator>MSBuild:Compile</Generator>
    </None>
    <None Update="Window1.xaml">
      <Generator>MSBuild:Compile</Generator>
    </None>
  </ItemGroup>
</Project>